{
  "name": "port-selector",
  "version": "0.1.5",
  "description": "a cross-platform Node library written in TypeScript and implemented in Rust. It mainly provides port availability checking and filtering ports based on filter conditions.",
  "keywords": [
    "port",
    "check",
    "select"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "name": "port-selector",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "commit": "git-cz",
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "jest",
    "lint:script": "eslint --ext .js,.ts --fix --quiet ./",
    "husky": "husky install"
  },
  "author": "ZingerLittleBee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZingerLittleBee/port-selector.git"
  },
  "bugs": {
    "url": "https://github.com/ZingerLittleBee/port-selector/issues"
  },
  "homepage": "https://github.com/ZingerLittleBee/port-selector",
  "devDependencies": {
    "@napi-rs/cli": "2.9.0",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/jest": "28.1.0",
    "@types/node": "17.0.30",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "commitizen": "4.2.4",
    "commitlint": "16.3.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "git-cz": "4.9.0",
    "husky": "7.0.4",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.2",
    "ts-node": "10.8.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "npm run lint:script",
      "git add --force"
    ]
  }
}